{
  "rewrites": [
    {
      "source": "/((?!assets|manifest\\.json|robots\\.txt|sitemap\\.xml|favicon\\.ico|.*\\.(jpg|jpeg|png|gif|svg|webp|ico|css|js|json|woff|woff2|ttf|eot)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
